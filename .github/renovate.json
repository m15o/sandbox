{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^debian.yaml$"
      ],
      "matchStrings": [
        "base-image:\\s*(?<currentValue>.*?)\n"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "debian",
      "versioningTemplate": "docker"
    }
  ]
}
